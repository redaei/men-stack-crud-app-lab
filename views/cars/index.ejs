<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Cars</title>
  </head>
  <body>
    <h1>Cars List</h1>
    <ul>
      <% cars.forEach((car) => { %>
      <li>
        <a href="/cars/<%= car._id %>"><%= car.model%> <%= car.make %> </a>
      </li>
      <% }); %>
    </ul>
    <a href="/cars/new">Add a Car</a>
  </body>
</html>
